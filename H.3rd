  <script>
    angular.module("myapp", [])
      .controller("MyController", function($scope) {
	  $scope.commondisabled = true;
      $scope.states = ["USA", "IND", "ENG"];
	    $scope.checkboxDisable= function(all)
	   {
	   if(all){
	    $scope.commondisabled = false;
	   }
	   else{
	     $scope.commondisabled = true;
		$scope.checkdisable=false;
	   $scope.checkdisable=false;
	   }
	console.log(all);
	   }
      });
	  
	   
	  
	   
	   
  </script>
</head>


